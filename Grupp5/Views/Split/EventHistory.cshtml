@{ViewBag.Title = "Avslutade event";}
@model ListOfEventsVM[]



<h2 class="eventHeading">Avslutade event</h2>
@if (Model.Count() == 0)
{
	<p class="message">Du har inga avslutade event</p>
}
@foreach (ListOfEventsVM item in Model)
{
    <a class="button" asp-controller="Split" asp-action="Overview" asp-route-id="@item.Id">@item.EventName</a>
}
