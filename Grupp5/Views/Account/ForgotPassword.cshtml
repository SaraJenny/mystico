@model ForgotPasswordVM

<h2>Återställ lösenord</h2>
<form asp-controller="Account" asp-action="ForgotPassword" >
	<label asp-for="Email"></label>
    <input asp-for="Email" />
	<span asp-validation-for="Email" id="emailError"></span>
    <input type="submit" value="Skicka" id="sendResetPasswordEmailButton"/>
</form>